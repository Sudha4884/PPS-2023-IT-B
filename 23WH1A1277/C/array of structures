#include<stdio.h>
#include<string.h>
void main(){
int i;
struct student
{ 
char name[10];
int id;
float marks;
}s[10];
for (i=0; i<10; i++)
{
printf("enter details: \n");
scanf("%s%d%f",s[i].name,&s[i].id,&s[i].marks);
}
for (i=0; i<10; i++)
{
printf("student details: \n name : %s \n id : %d \n marks : %f \n",s[i].name,s[i].id,s[i].marks);
}
}
