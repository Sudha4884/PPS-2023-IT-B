#include<stdio.h>
int main()
{
    int T[10][10],i,j,r,c;
    printf("enter number of rows : ");
    scanf("%d",&r);
    printf("enter number of coloums : ");
    scanf("%d",&c);
    printf("enter elements: ");
    for (i=0; i<r; i++)
    {
        for (j=0; j<c; j++)
        {
           scanf(" %d ", &T[i][j]); 
        }
    }
    printf("original matrix : \n");
     for (i=0; i<r; i++)
    {
        for (j=0; j<c; j++)
        {
           printf(" %d ", T[i][j]); 
        }
        printf("\n");
    }
    printf("\n *********** \n");
    printf("transpose of matrix : \n");
     for (i=0; i<r; i++)
    {
        for (j=0; j<c; j++)
        {
            printf(" %d ",T[j][i]);
        }
        printf("\n");
    }
}
