#include<stdio.h>
int main()
{
    int a,rem,sum,i,b,c;
    printf("Enter number: ");
    scanf("%d",&a);
    b = a;
    sum = 0;
    while(b!=0)
    {
        rem = b % 10;
        sum = sum * 10 + rem;
        b = b/10;
    }
    if (sum==a)
    {
        printf("this is a palindrome number");
    }
    else
    {
        printf("nope. not today.");
    }
    
}
