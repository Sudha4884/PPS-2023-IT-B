R = int(input("enter number of rows : "))
C = int(input("enter number of columns : "))
M =[]
print("enter the  " " elements : ".format(R*C))
for i in range(R):
  a = []
  for j in range(C):
    a.append(int(input()))
  M.append(a)

for i in range(R):
    for j in range(C):
        print(M[i][j], end = " ")
    print()
