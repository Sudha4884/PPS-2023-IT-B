from itertools import combinations
from itertools import permutations
str1 = str(input("Enter a string : "))
lst = []
for i in range(1,len(str1)):
  for j in combinations(str1,i):
    lst.append("".join(j))
print(lst)

lst = []
for i in range(1,len(str1)):
  for j in permutations(str1,i):
    lst.append("".join(j))
print(lst)
