//Swaping two numbers//
 #include<stdio.h>
int main(){
int a,b,c;
printf("enter two numbers to be swapped: ");
scanf("%d%d",&a,&b);
printf("a is %d \n",a);
printf("b is %d \n",b);
c=a;
a=b;
b=c;
printf("The numbers after swapping are: ");
printf("a is %d \n",a);
printf("b is %d \n",b); 

//method 2//
int m,l;
printf("enter two numbers to be swapped: ");
scanf("%d%d",&m,&l);
m=(m+l)-m;
l=(m+l)-m;
printf("the swapped numbers are:");
printf("\n m is %d",m);
printf("\n l is %d",l);

//method 3//
int x,y;
printf("enter two numbers to be swapped: ");
scanf("%d%d",&x,&y);
x=x+y;
y=x-y;
x=x-y;
printf("swapped numbers are: %d,%d",x,y);

//Method 4//
int f,d;
printf("enter two numbers to be swapped: ");
scanf("%d%d",&f,&d);
f=f*d;
d=f/d;
f=f/d;
printf("swapped numbers are: %d,%d",f,d);

//method 5//
int o,p;
printf("enter two numbers to be swapped: ");
scanf("%d%d",&o,&p);
p=o^p;
o=o^p;
p=o^p;
printf("swapped numbers are: %d,%d",o,p);
}
