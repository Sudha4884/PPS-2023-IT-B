//To find prime number between 1 to n. 
#include<stdio.h>
void main()
{
int i, num, n, count;
printf("Enter the range: ");
scanf("%d", &n);
printf("The prime numbers in between the range 1 to %d:\n",n);
for(num = 1;num<=n;num++)
{
count = 0;
for(i=2;i<=num/2;i++)
{
if(num%i==0)
{
count++;
}
}
if(count==0 && num!= 1)
printf("%d ",num);
}
}
