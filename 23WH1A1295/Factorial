//To find a factorial for a given number. 
#include <stdio.h>

int factorial(int num);

void main()
{
	int num,factorial;
 
  	printf("Enter a number to calculate it's factorial\n");
  	scanf("%d",&num);
  	factorial = fact (num) ;
    printf("Factorial of the num(%d) = %d\n",num,factorial);
}

int fact(int n)
{
    int i,f=1;
    for(i=1;i<=n;i++)
    {
        f=f*i;
    }
    return f;
}
